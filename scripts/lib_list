#!/bin/bash
#
#   This file contains different power-voltage libary paths 
#

# Clock Libs
export CLK_LIB_GPLVT=$STM065_DIR/CLOCK65GPLVT_3.1/libs
export CLK_LIB_GPSVT=$STM065_DIR/CLOCK65GPSVT_3.1/libs
export CLK_LIB_LPHVT=$STM065_DIR/CLOCK65LPHVT_3.1/libs
export CLK_LIB_LPLVT=$STM065_DIR/CLOCK65LPLVT_3.1/libs
export CLK_LIB_LPSVT=$STM065_DIR/CLOCK65LPSVT_3.1/libs
# Buffer Libs
export BUF_LIB_GPLVT=$STM065_DIR/CORE65GPLVT_5.1/libs
export BUF_LIB_GPSVT=$STM065_DIR/CORE65GPSVT_5.1/libs
export BUF_LIB_LPHVT=$STM065_DIR/CORE65LPHVT_5.1/libs
export BUF_LIB_LPLVT=$STM065_DIR/CORE65LPLVT_5.1/libs
export BUF_LIB_LPSVT=$STM065_DIR/CORE65LPSVT_5.1/libs

